meta {
  name: Login
  type: http
  seq: 2
}

post {
  url: {{baseUrl}}/auth/login
  body: json
  auth: none
}

body:json {
  {
    "email":"{{systemAdminEmail}}",
    "password":"{{systemAdminPassword}}"
  }
}

script:post-response {
  bru.setEnvVar('accessToken',res.body?.access?.token)
  bru.setEnvVar('refreshToken',res.body?.refresh?.token)
}
